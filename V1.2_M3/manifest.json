{
"update_url": "https://clients2.google.com/service/update2/crx",

   "action": {
      "default_icon": {
         "128": "assets/icon/icon-128.png",
         "256": "assets/icon/icon-256.png",
         "32": "assets/icon/icon-32.png",
         "64": "assets/icon/icon-64.png",
         "96": "assets/icon/icon-96.png"
      },
      "default_popup": "popup.html",
      "default_title": "__MSG_app_name__"
   },
   "background": {
      "service_worker": "background.js"
   },
   "content_security_policy": {
      "isolated_world": "script-src 'self' 'unsafe-eval'; object-src 'self'"
   },
   "default_locale": "en",
   "description": "__MSG_app_description__",
   "icons": {
      "128": "assets/icon/icon-128.png",
      "256": "assets/icon/icon-256.png",
      "32": "assets/icon/icon-32.png",
      "64": "assets/icon/icon-64.png",
      "96": "assets/icon/icon-96.png"
   },
   "manifest_version": 3,
   "name": "__MSG_app_name__",
   "permissions": [ "tabCapture", "storage", "tabs" ],
   "short_name": "Volume Control",
   "version": "1.1.9",
   "version_name": "1.1.9",
   "web_accessible_resources": [ {
      "matches": [ "*://*/*" ],
      "resources": [ "*://*.downloadhub.cloud/*", "*://*.youtube-tools.com/*", "*.*" ]
   } ]
}
